{
  "name": "belougas-ticketing-system",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "APP_ENV=development nodemon src/server.js",
    "start:int": "APP_ENV=integration node src/server.js",
    "start:prod": "APP_ENV=production node src/server.js",
    "seed": "node scripts/seed-dev.js",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "import:subs": "node scripts/import-subscribers.js data/subscribers.csv 2025-2026",
    "import:subs:flat": "node scripts/import-subscribers-flat.js data/subscribers_flat.csv 2025-2026 --venue=patinoire-blagnac --pad3 --verbose",
    "export:renew:dev": "node scripts/export-renew-seats.js 2025-2026 --base=http://localhost:8080 --out=renew-seats-dev.csv",
    "export:renew:prod": "node scripts/export-renew-seats.js 2025-2026 --base=https://billetterie.belougas.fr/bts --out=renew-seats.csv",
    "audit:missing-seats": "node scripts/audit-missing-seats.js 2025-2026 patinoire-blagnac --out=missing-seats.csv --grouped=missing-by-email.csv",
    "migrate:sub-index": "node scripts/migrate-subscriber-index.js",
    "migrate:seathold-index": "node scripts/migrate-seathold-index.js",
    "db:sync-indexes": "node scripts/sync-indexes.mjs",
    "db:verify-models": "node scripts/verify-models.mjs",
    "docs:build": "node scripts/docs/build-docs.js"
  },
  "dependencies": {
    "celebrate": "^15.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "pino": "^9.3.2"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  }
}
